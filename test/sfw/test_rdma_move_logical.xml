    <subtest>
      <desc>Test PUT with volatile MD</desc>
      <arg_check>
        <ptl>
          <ptl_ni ni_opt="MATCH PHYSICAL">
            <ompi_rt>
              <ptl_ni ni_opt="MATCH LOGICAL" desired_max_volatile_size="512">
                <ptl_eq>
                  <ptl_pt>
                    <ptl_get_id>
                      
                      <if rank="0">
                        <ptl_md md_data="0xaa" md_opt="VOLATILE" md_length="10000">
                          
                          <!-- Wait for rank 1 to set up the receiving buffer -->
                          <barrier/>
                          
                          <!-- max_volatile_size is 512 bytes, so any transfer below must fail. -->
                          <ptl_put ack_req="ACK" length="9000" match="0x5555" target_id="1" ret="ARG_INVALID"/>
                          <ptl_put ack_req="ACK" length="513" match="0x5555" target_id="1" ret="ARG_INVALID"/>
                          <ptl_put ack_req="ACK" length="512" match="0x5555" target_id="1" ret="OK"/>
                          
                          
                          <!-- Wait for the packet containing the ack -->
                          <ptl_eq_wait>
                            <check event_type="SEND"/>
                          </ptl_eq_wait>
                          
                          <!-- Wait for the ack -->
                          <ptl_eq_wait>
                            <check event_type="ACK"/>
                            <check event_ni_fail="OK"/>
                          </ptl_eq_wait>
                          
                          <ptl_put ack_req="ACK" length="511" match="0x5555" target_id="1" ret="OK"/>
                          
                          
                          <!-- Wait for the packet containing the ack -->
                          <ptl_eq_wait>
                            <check event_type="SEND"/>
                          </ptl_eq_wait>
                          
                          <!-- Wait for the ack -->
                          <ptl_eq_wait>
                            <check event_type="ACK"/>
                            <check event_ni_fail="OK"/>
                          </ptl_eq_wait>
                          
                          
                          <barrier/>
                          
                        </ptl_md>
                      </if>
                      
                      <if rank="1">
                        <ptl_me me_opt="OP_PUT EVENT_LINK_DISABLE" me_match="0x5555" me_data="0x77">
                          
                          <!-- Force rank 0 to wait for the buffer to be ready -->
                          <barrier/>
                          
                          <!-- Wait for rank 0 to coalesce -->
                          <barrier/>
                          
                        </ptl_me>
                      </if>
                      
                    </ptl_get_id>
                  </ptl_pt>
                </ptl_eq>
              </ptl_ni>
            </ompi_rt>
          </ptl_ni>
        </ptl>
      </arg_check>
    </subtest>
    
    <subtest>
      <desc>Test PtlGet with good parameters from ME, on overflow list</desc>
      <ptl>
        <ptl_ni>
          <ompi_rt>
            <ptl_ni desired_max_msg_size="8192" ni_opt="MATCH LOGICAL">
              <ptl_eq>
                <ptl_pt>
                  <ptl_get_id>
                    
                    <if rank="0">
                      <ptl_md md_length="8192" type="UINT" me_data="0x00000000">
                        <barrier/>
                        <ptl_get length="8192" target_id="1" match="0x5555"/>
                        <ptl_eq_poll time="200">
                          <check event_type="REPLY"/>
                          <check event_ptl_list="OVERFLOW"/>
                        </ptl_eq_poll>
                        <check length="8192" type="UINT" md_data="0x5a5a5a5a"/>
                        <barrier/>
                      </ptl_md>
                    </if>
                    
                    <if rank="1">
                      <ptl_me me_opt="OP_GET EVENT_LINK_DISABLE" me_length="8192" me_match="0x5555" type="UINT" me_data="0x5a5a5a5a" list="OVERFLOW">
                        <barrier/>
                        <ptl_eq_poll time="200">
                          <check event_type="GET"/>
                        </ptl_eq_poll>
                        <barrier/>
                        
                        <!-- We're not doing an append, so search and
                             delete the entry from the unexpected
                             list, else ptl_me will never
                             terminate. -->
                        <ptl_me_search me_opt="OP_GET" me_match="0x5555" search_op="DELETE">
                          
                          <!-- Wait for PTL_NI_OK event -->
                          <ptl_eq_wait>
                            <check event_type="GET_OVERFLOW"/>
                          </ptl_eq_wait>
                          
                        </ptl_me_search>
                        
                      </ptl_me>
                    </if>
                    
                  </ptl_get_id>
                </ptl_pt>
              </ptl_eq>
            </ptl_ni>
          </ompi_rt>
        </ptl_ni>
      </ptl>
    </subtest>
    
  </repeat>
</test>

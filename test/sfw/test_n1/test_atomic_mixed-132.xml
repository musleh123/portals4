<?xml version="1.0"?>
<test>
  <desc>Test atomic PROD/UCHAR length=7</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0x9a" le_opt="OP_PUT" type="UCHAR">
          <ptl_md md_data="0xff" type="UCHAR">
            <ptl_atomic atom_op="PROD" atom_type="UCHAR" length="7" target_id="SELF"/>
            <msleep count="50"/>
            <check length="7" md_data="0xff" type="UCHAR"/>
            <check length="1" md_data="0xff" offset="7" type="UCHAR"/>
          </ptl_md>
          <check le_data="0x66" length="7" type="UCHAR"/>
          <check le_data="0x9a" length="1" offset="7" type="UCHAR"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

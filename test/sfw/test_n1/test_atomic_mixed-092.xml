<?xml version="1.0"?>
<test>
  <desc>Test atomic LAND/INT length=12</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0x1a80c3fd" le_opt="OP_PUT" type="INT">
          <ptl_md md_data="0x1c5727fa" type="INT">
            <ptl_atomic atom_op="LAND" atom_type="INT" length="12" target_id="SELF"/>
            <msleep count="50"/>
            <check length="12" md_data="0x1c5727fa" type="INT"/>
            <check length="4" md_data="0x1c5727fa" offset="12" type="INT"/>
          </ptl_md>
          <check le_data="0x00000001" length="12" type="INT"/>
          <check le_data="0x1a80c3fd" length="4" offset="12" type="INT"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

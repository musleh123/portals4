<?xml version="1.0"?>
<test>
  <desc>Test atomic MAX/UCHAR length=6</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0x39" le_opt="OP_PUT" type="UCHAR">
          <ptl_md md_data="0xb4" type="UCHAR">
            <ptl_atomic atom_op="MAX" atom_type="UCHAR" length="6" target_id="SELF"/>
            <msleep count="50"/>
            <check length="6" md_data="0xb4" type="UCHAR"/>
            <check length="1" md_data="0xb4" offset="6" type="UCHAR"/>
          </ptl_md>
          <check le_data="0xb4" length="6" type="UCHAR"/>
          <check le_data="0x39" length="1" offset="6" type="UCHAR"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

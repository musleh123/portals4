<?xml version="1.0"?>
<test>
  <desc>Test atomic SUM/SHORT length=20</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0x9335" le_opt="OP_PUT" type="SHORT">
          <ptl_md md_data="0x8eea" type="SHORT">
            <ptl_atomic atom_op="SUM" atom_type="SHORT" length="20" target_id="SELF"/>
            <msleep count="50"/>
            <check length="20" md_data="0x8eea" type="SHORT"/>
            <check length="2" md_data="0x8eea" offset="20" type="SHORT"/>
          </ptl_md>
          <check le_data="0x221f" length="20" type="SHORT"/>
          <check le_data="0x9335" length="2" offset="20" type="SHORT"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

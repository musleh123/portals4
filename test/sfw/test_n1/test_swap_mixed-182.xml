<?xml version="1.0"?>
<test>
  <desc>Test swap CSWAP_LE/CHAR</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0xe5" le_opt="OP_PUT OP_GET" type="CHAR">
          <ptl_md md_data="0xb0" type="CHAR">
            <ptl_md md_data="0x37" type="CHAR">
              <ptl_swap atom_op="CSWAP_LE" atom_type="CHAR" length="1" operand="0x98" target_id="SELF"/>
              <msleep count="50"/>
              <check length="1" md_data="0xe5" type="CHAR"/>
              <check length="1" md_data="0x37" offset="1" type="CHAR"/>
            </ptl_md>
            <check length="1" md_data="0xb0" type="CHAR"/>
            <check length="1" md_data="0xb0" offset="1" type="CHAR"/>
          </ptl_md>
          <check le_data="0xb0" length="1" type="CHAR"/>
          <check le_data="0xe5" length="1" offset="1" type="CHAR"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

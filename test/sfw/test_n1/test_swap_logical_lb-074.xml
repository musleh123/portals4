<?xml version="1.0"?>
<test>
  <desc>Test swap CSWAP_LE/UCHAR</desc>
  <ptl>
    <ptl_ni>
      <ompi_rt>
        <ptl_ni ni_opt="NO_MATCH LOGICAL">
          <ptl_pt>
            <ptl_le le_data="0xbb" le_opt="OP_PUT OP_GET" type="UCHAR">
              <ptl_md md_data="0x77" type="UCHAR">
                <ptl_md md_data="0xf7" type="UCHAR">
                  <ptl_swap atom_op="CSWAP_LE" atom_type="UCHAR" length="1" operand="0x41" target_id="SELF"/>
                  <msleep count="50"/>
                </ptl_md>
                <check length="1" md_data="0x77" type="UCHAR"/>
                <check length="1" md_data="0x77" offset="1" type="UCHAR"/>
              </ptl_md>
              <check le_data="0x77" length="1" type="UCHAR"/>
              <check le_data="0xbb" length="1" offset="1" type="UCHAR"/>
            </ptl_le>
          </ptl_pt>
          <barrier/>
        </ptl_ni>
      </ompi_rt>
    </ptl_ni>
  </ptl>
</test>

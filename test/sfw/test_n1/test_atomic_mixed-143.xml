<?xml version="1.0"?>
<test>
  <desc>Test atomic BXOR/CHAR length=21</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0x14" le_opt="OP_PUT" type="CHAR">
          <ptl_md md_data="0x46" type="CHAR">
            <ptl_atomic atom_op="BXOR" atom_type="CHAR" length="21" target_id="SELF"/>
            <msleep count="50"/>
            <check length="21" md_data="0x46" type="CHAR"/>
            <check length="1" md_data="0x46" offset="21" type="CHAR"/>
          </ptl_md>
          <check le_data="0x52" length="21" type="CHAR"/>
          <check le_data="0x14" length="1" offset="21" type="CHAR"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

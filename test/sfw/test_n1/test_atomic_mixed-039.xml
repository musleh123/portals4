<?xml version="1.0"?>
<test>
  <desc>Test atomic LOR/UINT length=4</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0x67fa681b" le_opt="OP_PUT" type="UINT">
          <ptl_md md_data="0x37eb6e91" type="UINT">
            <ptl_atomic atom_op="LOR" atom_type="UINT" length="4" target_id="SELF"/>
            <msleep count="50"/>
            <check length="4" md_data="0x37eb6e91" type="UINT"/>
            <check length="4" md_data="0x37eb6e91" offset="4" type="UINT"/>
          </ptl_md>
          <check le_data="0x00000001" length="4" type="UINT"/>
          <check le_data="0x67fa681b" length="4" offset="4" type="UINT"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

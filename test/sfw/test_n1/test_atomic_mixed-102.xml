<?xml version="1.0"?>
<test>
  <desc>Test atomic LXOR/CHAR length=6</desc>
  <ptl>
    <ptl_ni ni_opt="MATCH PHYSICAL">
      <ptl_pt>
        <ptl_me me_data="0x4b" me_match="0x7886595465436710988" me_opt="OP_PUT" type="CHAR">
          <ptl_md md_data="0x45" type="CHAR">
            <ptl_atomic atom_op="LXOR" atom_type="CHAR" length="6" match="0x7886595465436710988" target_id="SELF"/>
            <msleep count="50"/>
            <check length="6" md_data="0x45" type="CHAR"/>
            <check length="1" md_data="0x45" offset="6" type="CHAR"/>
          </ptl_md>
          <check length="6" me_data="0x00" type="CHAR"/>
          <check length="1" me_data="0x4b" offset="6" type="CHAR"/>
        </ptl_me>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

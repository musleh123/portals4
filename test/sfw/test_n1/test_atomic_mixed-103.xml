<?xml version="1.0"?>
<test>
  <desc>Test atomic LXOR/UCHAR length=27</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0xeb" le_opt="OP_PUT" type="UCHAR">
          <ptl_md md_data="0x26" type="UCHAR">
            <ptl_atomic atom_op="LXOR" atom_type="UCHAR" length="27" target_id="SELF"/>
            <msleep count="50"/>
            <check length="27" md_data="0x26" type="UCHAR"/>
            <check length="1" md_data="0x26" offset="27" type="UCHAR"/>
          </ptl_md>
          <check le_data="0x00" length="27" type="UCHAR"/>
          <check le_data="0xeb" length="1" offset="27" type="UCHAR"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

<?xml version="1.0"?>
<test>
  <desc>Test atomic MAX/UCHAR length=11</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0xaa" le_opt="OP_PUT" type="UCHAR">
          <ptl_md md_data="0x3b" type="UCHAR">
            <ptl_atomic atom_op="MAX" atom_type="UCHAR" length="11" target_id="SELF"/>
            <msleep count="50"/>
            <check length="11" md_data="0x3b" type="UCHAR"/>
            <check length="1" md_data="0x3b" offset="11" type="UCHAR"/>
          </ptl_md>
          <check le_data="0xaa" length="11" type="UCHAR"/>
          <check le_data="0xaa" length="1" offset="11" type="UCHAR"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

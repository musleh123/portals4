<?xml version="1.0"?>
<test>
  <desc>Test atomic LOR/INT8 length=5</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0x52" le_opt="OP_PUT" type="INT8">
          <ptl_md md_data="0x54" type="INT8">
            <ptl_atomic atom_op="LOR" atom_type="INT8" length="5" target_id="SELF"/>
            <msleep count="50"/>
            <check length="5" md_data="0x54" type="INT8"/>
            <check length="1" md_data="0x54" offset="5" type="INT8"/>
          </ptl_md>
          <check le_data="0x01" length="5" type="INT8"/>
          <check le_data="0x52" length="1" offset="5" type="INT8"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

      <ptl_pt>
        <ptl_le le_data="0x36cb" le_opt="OP_PUT" type="USHORT">
          <ptl_md md_data="0x4e6f" type="USHORT">
            <ptl_atomic atom_op="BXOR" atom_type="USHORT" length="12" target_id="SELF"/>
            <msleep count="50"/>
            <check length="12" md_data="0x4e6f" type="USHORT"/>
            <check length="2" md_data="0x4e6f" offset="12" type="USHORT"/>
          </ptl_md>
          <check le_data="0x78a4" length="12" type="USHORT"/>
          <check le_data="0x36cb" length="2" offset="12" type="USHORT"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

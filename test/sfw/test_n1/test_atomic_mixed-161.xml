<?xml version="1.0"?>
<test>
  <desc>Test atomic LXOR/USHORT length=32</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0x1dfb" le_opt="OP_PUT" type="USHORT">
          <ptl_md md_data="0xe8c2" type="USHORT">
            <ptl_atomic atom_op="LXOR" atom_type="USHORT" length="32" target_id="SELF"/>
            <msleep count="50"/>
            <check length="32" md_data="0xe8c2" type="USHORT"/>
            <check length="2" md_data="0xe8c2" offset="32" type="USHORT"/>
          </ptl_md>
          <check le_data="0x0000" length="32" type="USHORT"/>
          <check le_data="0x1dfb" length="2" offset="32" type="USHORT"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

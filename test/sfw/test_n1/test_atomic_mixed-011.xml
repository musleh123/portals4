<?xml version="1.0"?>
<test>
  <desc>Test atomic MIN/CHAR length=23</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0xa5" le_opt="OP_PUT" type="CHAR">
          <ptl_md md_data="0xfe" type="CHAR">
            <ptl_atomic atom_op="MIN" atom_type="CHAR" length="23" target_id="SELF"/>
            <msleep count="50"/>
            <check length="23" md_data="0xfe" type="CHAR"/>
            <check length="1" md_data="0xfe" offset="23" type="CHAR"/>
          </ptl_md>
          <check le_data="0xa5" length="23" type="CHAR"/>
          <check le_data="0xa5" length="1" offset="23" type="CHAR"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

<?xml version="1.0"?>
<test>
  <desc>Test atomic PROD/UCHAR length=31</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0x78" le_opt="OP_PUT" type="UCHAR">
          <ptl_md md_data="0x39" type="UCHAR">
            <ptl_atomic atom_op="PROD" atom_type="UCHAR" length="31" target_id="SELF"/>
            <msleep count="50"/>
            <check length="31" md_data="0x39" type="UCHAR"/>
            <check length="1" md_data="0x39" offset="31" type="UCHAR"/>
          </ptl_md>
          <check le_data="0xb8" length="31" type="UCHAR"/>
          <check le_data="0x78" length="1" offset="31" type="UCHAR"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

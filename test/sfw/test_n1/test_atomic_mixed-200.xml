<?xml version="1.0"?>
<test>
  <desc>Test atomic LXOR/UCHAR length=26</desc>
  <ptl>
    <ptl_ni ni_opt="MATCH PHYSICAL">
      <ptl_pt>
        <ptl_me me_data="0x66" me_match="0x4092536799629493595" me_opt="OP_PUT" type="UCHAR">
          <ptl_md md_data="0x2d" type="UCHAR">
            <ptl_atomic atom_op="LXOR" atom_type="UCHAR" length="26" match="0x4092536799629493595" target_id="SELF"/>
            <msleep count="50"/>
            <check length="26" md_data="0x2d" type="UCHAR"/>
            <check length="1" md_data="0x2d" offset="26" type="UCHAR"/>
          </ptl_md>
          <check length="26" me_data="0x00" type="UCHAR"/>
          <check length="1" me_data="0x66" offset="26" type="UCHAR"/>
        </ptl_me>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

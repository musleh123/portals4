<?xml version="1.0"?>
<test>
  <desc>Test atomic SUM/CHAR length=13</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0xef" le_opt="OP_PUT" type="CHAR">
          <ptl_md md_data="0x50" type="CHAR">
            <ptl_atomic atom_op="SUM" atom_type="CHAR" length="13" target_id="SELF"/>
            <msleep count="50"/>
            <check length="13" md_data="0x50" type="CHAR"/>
            <check length="1" md_data="0x50" offset="13" type="CHAR"/>
          </ptl_md>
          <check le_data="0x3f" length="13" type="CHAR"/>
          <check le_data="0xef" length="1" offset="13" type="CHAR"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

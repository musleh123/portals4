<?xml version="1.0"?>
<test>
  <desc>Test atomic BAND/CHAR length=15</desc>
  <ptl>
    <ptl_ni ni_opt="MATCH PHYSICAL">
      <ptl_pt>
        <ptl_me me_data="0x5d" me_match="0x6421877610251957814" me_opt="OP_PUT" type="CHAR">
          <ptl_md md_data="0xb7" type="CHAR">
            <ptl_atomic atom_op="BAND" atom_type="CHAR" length="15" match="0x6421877610251957814" target_id="SELF"/>
            <msleep count="50"/>
            <check length="15" md_data="0xb7" type="CHAR"/>
            <check length="1" md_data="0xb7" offset="15" type="CHAR"/>
          </ptl_md>
          <check length="15" me_data="0x15" type="CHAR"/>
          <check length="1" me_data="0x5d" offset="15" type="CHAR"/>
        </ptl_me>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

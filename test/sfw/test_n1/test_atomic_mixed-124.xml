<?xml version="1.0"?>
<test>
  <desc>Test atomic LXOR/INT32 length=20</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0x6d8ce060" le_opt="OP_PUT" type="INT32">
          <ptl_md md_data="0x16676ad3" type="INT32">
            <ptl_atomic atom_op="LXOR" atom_type="INT32" length="20" target_id="SELF"/>
            <msleep count="50"/>
            <check length="20" md_data="0x16676ad3" type="INT32"/>
            <check length="4" md_data="0x16676ad3" offset="20" type="INT32"/>
          </ptl_md>
          <check le_data="0x00000000" length="20" type="INT32"/>
          <check le_data="0x6d8ce060" length="4" offset="20" type="INT32"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

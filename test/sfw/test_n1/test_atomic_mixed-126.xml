<?xml version="1.0"?>
<test>
  <desc>Test atomic MAX/FLOAT length=8</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0.4983376265" le_opt="OP_PUT" type="FLOAT">
          <ptl_md md_data="0.1660847664" type="FLOAT">
            <ptl_atomic atom_op="MAX" atom_type="FLOAT" length="8" target_id="SELF"/>
            <msleep count="50"/>
            <check length="8" md_data="0.1660847664" type="FLOAT"/>
            <check length="4" md_data="0.1660847664" offset="8" type="FLOAT"/>
          </ptl_md>
          <check le_data="0.4983376265" length="8" type="FLOAT"/>
          <check le_data="0.4983376265" length="4" offset="8" type="FLOAT"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

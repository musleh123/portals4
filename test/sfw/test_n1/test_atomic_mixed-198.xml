<?xml version="1.0"?>
<test>
  <desc>Test atomic MAX/UINT8 length=32</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0xdd" le_opt="OP_PUT" type="UINT8">
          <ptl_md md_data="0x9d" type="UINT8">
            <ptl_atomic atom_op="MAX" atom_type="UINT8" length="32" target_id="SELF"/>
            <msleep count="50"/>
            <check length="32" md_data="0x9d" type="UINT8"/>
            <check length="1" md_data="0x9d" offset="32" type="UINT8"/>
          </ptl_md>
          <check le_data="0xdd" length="32" type="UINT8"/>
          <check le_data="0xdd" length="1" offset="32" type="UINT8"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

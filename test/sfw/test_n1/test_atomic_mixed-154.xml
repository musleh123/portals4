            <ptl_atomic atom_op="BXOR" atom_type="CHAR" length="2" match="0x801629047719096742" target_id="SELF"/>
            <msleep count="50"/>
            <check length="2" md_data="0x5a" type="CHAR"/>
            <check length="1" md_data="0x5a" offset="2" type="CHAR"/>
          </ptl_md>
          <check length="2" me_data="0x1c" type="CHAR"/>
          <check length="1" me_data="0x46" offset="2" type="CHAR"/>
        </ptl_me>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

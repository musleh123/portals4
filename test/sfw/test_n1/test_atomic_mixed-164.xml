<?xml version="1.0"?>
<test>
  <desc>Test atomic LOR/SHORT length=10</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0x6b04" le_opt="OP_PUT" type="SHORT">
          <ptl_md md_data="0xc8e7" type="SHORT">
            <ptl_atomic atom_op="LOR" atom_type="SHORT" length="10" target_id="SELF"/>
            <msleep count="50"/>
            <check length="10" md_data="0xc8e7" type="SHORT"/>
            <check length="2" md_data="0xc8e7" offset="10" type="SHORT"/>
          </ptl_md>
          <check le_data="0x0001" length="10" type="SHORT"/>
          <check le_data="0x6b04" length="2" offset="10" type="SHORT"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

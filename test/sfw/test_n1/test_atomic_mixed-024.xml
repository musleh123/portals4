<?xml version="1.0"?>
<test>
  <desc>Test atomic BOR/UINT8 length=4</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0x88" le_opt="OP_PUT" type="UINT8">
          <ptl_md md_data="0x87" type="UINT8">
            <ptl_atomic atom_op="BOR" atom_type="UINT8" length="4" target_id="SELF"/>
            <msleep count="50"/>
            <check length="4" md_data="0x87" type="UINT8"/>
            <check length="1" md_data="0x87" offset="4" type="UINT8"/>
          </ptl_md>
          <check le_data="0x8f" length="4" type="UINT8"/>
          <check le_data="0x88" length="1" offset="4" type="UINT8"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

<?xml version="1.0"?>
<test>
  <desc>Test atomic LOR/INT16 length=16</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0x3e05" le_opt="OP_PUT" type="INT16">
          <ptl_md md_data="0x4ce8" type="INT16">
            <ptl_atomic atom_op="LOR" atom_type="INT16" length="16" target_id="SELF"/>
            <msleep count="50"/>
            <check length="16" md_data="0x4ce8" type="INT16"/>
            <check length="2" md_data="0x4ce8" offset="16" type="INT16"/>
          </ptl_md>
          <check le_data="0x0001" length="16" type="INT16"/>
          <check le_data="0x3e05" length="2" offset="16" type="INT16"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

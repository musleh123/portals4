<?xml version="1.0"?>
<test>
  <desc>Test atomic LXOR/CHAR length=14</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0x42" le_opt="OP_PUT" type="CHAR">
          <ptl_md md_data="0x7b" type="CHAR">
            <ptl_atomic atom_op="LXOR" atom_type="CHAR" length="14" target_id="SELF"/>
            <msleep count="50"/>
            <check length="14" md_data="0x7b" type="CHAR"/>
            <check length="1" md_data="0x7b" offset="14" type="CHAR"/>
          </ptl_md>
          <check le_data="0x00" length="14" type="CHAR"/>
          <check le_data="0x42" length="1" offset="14" type="CHAR"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

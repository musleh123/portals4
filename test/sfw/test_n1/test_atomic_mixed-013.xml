<?xml version="1.0"?>
<test>
  <desc>Test atomic LOR/INT length=16</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0x18d481b5" le_opt="OP_PUT" type="INT">
          <ptl_md md_data="0x1724deb6" type="INT">
            <ptl_atomic atom_op="LOR" atom_type="INT" length="16" target_id="SELF"/>
            <msleep count="50"/>
            <check length="16" md_data="0x1724deb6" type="INT"/>
            <check length="4" md_data="0x1724deb6" offset="16" type="INT"/>
          </ptl_md>
          <check le_data="0x00000001" length="16" type="INT"/>
          <check le_data="0x18d481b5" length="4" offset="16" type="INT"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

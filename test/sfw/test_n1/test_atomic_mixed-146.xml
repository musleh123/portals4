<?xml version="1.0"?>
<test>
  <desc>Test atomic BAND/USHORT length=8</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0x359d" le_opt="OP_PUT" type="USHORT">
          <ptl_md md_data="0x4c9d" type="USHORT">
            <ptl_atomic atom_op="BAND" atom_type="USHORT" length="8" target_id="SELF"/>
            <msleep count="50"/>
            <check length="8" md_data="0x4c9d" type="USHORT"/>
            <check length="2" md_data="0x4c9d" offset="8" type="USHORT"/>
          </ptl_md>
          <check le_data="0x049d" length="8" type="USHORT"/>
          <check le_data="0x359d" length="2" offset="8" type="USHORT"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

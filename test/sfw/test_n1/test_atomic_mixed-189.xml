<?xml version="1.0"?>
<test>
  <desc>Test atomic BAND/INT8 length=8</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0xf5" le_opt="OP_PUT" type="INT8">
          <ptl_md md_data="0x4d" type="INT8">
            <ptl_atomic atom_op="BAND" atom_type="INT8" length="8" target_id="SELF"/>
            <msleep count="50"/>
            <check length="8" md_data="0x4d" type="INT8"/>
            <check length="1" md_data="0x4d" offset="8" type="INT8"/>
          </ptl_md>
          <check le_data="0x45" length="8" type="INT8"/>
          <check le_data="0xf5" length="1" offset="8" type="INT8"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

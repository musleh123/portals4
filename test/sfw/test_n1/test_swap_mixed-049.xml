<?xml version="1.0"?>
<test>
  <desc>Test swap CSWAP_LE/INT8</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0xce" le_opt="OP_PUT OP_GET" type="INT8">
          <ptl_md md_data="0x37" type="INT8">
            <ptl_md md_data="0x1d" type="INT8">
              <ptl_swap atom_op="CSWAP_LE" atom_type="INT8" length="1" operand="0x3f" target_id="SELF"/>
              <msleep count="50"/>
              <check length="1" md_data="0xce" type="INT8"/>
              <check length="1" md_data="0x1d" offset="1" type="INT8"/>
            </ptl_md>
            <check length="1" md_data="0x37" type="INT8"/>
            <check length="1" md_data="0x37" offset="1" type="INT8"/>
          </ptl_md>
          <check le_data="0xce" length="1" type="INT8"/>
          <check le_data="0xce" length="1" offset="1" type="INT8"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

              <ptl_swap atom_op="CSWAP_LT" atom_type="INT8" length="1" operand="0x6f" target_id="SELF"/>
              <msleep count="50"/>
              <check length="1" md_data="0x8c" type="INT8"/>
              <check length="1" md_data="0xe2" offset="1" type="INT8"/>
            </ptl_md>
            <check length="1" md_data="0xa2" type="INT8"/>
            <check length="1" md_data="0xa2" offset="1" type="INT8"/>
          </ptl_md>
          <check le_data="0x8c" length="1" type="INT8"/>
          <check le_data="0x8c" length="1" offset="1" type="INT8"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

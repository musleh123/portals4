<?xml version="1.0"?>
<test>
  <desc>Test swap CSWAP/USHORT</desc>
  <ptl>
    <ptl_ni>
      <ompi_rt>
        <ptl_ni ni_opt="NO_MATCH LOGICAL">
          <ptl_pt>
            <ptl_le le_data="0xafff" le_opt="OP_PUT OP_GET" type="USHORT">
              <ptl_md md_data="0xd569" type="USHORT">
                <ptl_md md_data="0xec79" type="USHORT">
                  <ptl_swap atom_op="CSWAP" atom_type="USHORT" length="2" operand="0x7aab" target_id="SELF"/>
                  <msleep count="50"/>
                </ptl_md>
                <check length="2" md_data="0xd569" type="USHORT"/>
                <check length="2" md_data="0xd569" offset="2" type="USHORT"/>
              </ptl_md>
              <check le_data="0xafff" length="2" type="USHORT"/>
              <check le_data="0xafff" length="2" offset="2" type="USHORT"/>
            </ptl_le>
          </ptl_pt>
          <barrier/>
        </ptl_ni>
      </ompi_rt>
    </ptl_ni>
  </ptl>
</test>

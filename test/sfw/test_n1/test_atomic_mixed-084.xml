<?xml version="1.0"?>
<test>
  <desc>Test atomic LXOR/INT8 length=1</desc>
  <ptl>
    <ptl_ni ni_opt="MATCH PHYSICAL">
      <ptl_pt>
        <ptl_me me_data="0xbf" me_match="0x6057131755264776420" me_opt="OP_PUT" type="INT8">
          <ptl_md md_data="0xf2" type="INT8">
            <ptl_atomic atom_op="LXOR" atom_type="INT8" length="1" match="0x6057131755264776420" target_id="SELF"/>
            <msleep count="50"/>
            <check length="1" md_data="0xf2" type="INT8"/>
            <check length="1" md_data="0xf2" offset="1" type="INT8"/>
          </ptl_md>
          <check length="1" me_data="0x00" type="INT8"/>
          <check length="1" me_data="0xbf" offset="1" type="INT8"/>
        </ptl_me>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

<?xml version="1.0"?>
<test>
  <desc>Test swap CSWAP_LT/CHAR</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0x26" le_opt="OP_PUT OP_GET" type="CHAR">
          <ptl_md md_data="0x74" type="CHAR">
            <ptl_md md_data="0x41" type="CHAR">
              <ptl_swap atom_op="CSWAP_LT" atom_type="CHAR" length="1" operand="0xff" target_id="SELF"/>
              <msleep count="50"/>
              <check length="1" md_data="0x26" type="CHAR"/>
              <check length="1" md_data="0x41" offset="1" type="CHAR"/>
            </ptl_md>
            <check length="1" md_data="0x74" type="CHAR"/>
            <check length="1" md_data="0x74" offset="1" type="CHAR"/>
          </ptl_md>
          <check le_data="0x74" length="1" type="CHAR"/>
          <check le_data="0x26" length="1" offset="1" type="CHAR"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

<?xml version="1.0"?>
<test>
  <desc>Test atomic MIN/INT8 length=16</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0x56" le_opt="OP_PUT" type="INT8">
          <ptl_md md_data="0xc3" type="INT8">
            <ptl_atomic atom_op="MIN" atom_type="INT8" length="16" target_id="SELF"/>
            <msleep count="50"/>
            <check length="16" md_data="0xc3" type="INT8"/>
            <check length="1" md_data="0xc3" offset="16" type="INT8"/>
          </ptl_md>
          <check le_data="0xc3" length="16" type="INT8"/>
          <check le_data="0x56" length="1" offset="16" type="INT8"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

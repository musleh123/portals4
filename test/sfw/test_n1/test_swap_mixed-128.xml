<?xml version="1.0"?>
<test>
  <desc>Test swap CSWAP_LE/CHAR</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0x6d" le_opt="OP_PUT OP_GET" type="CHAR">
          <ptl_md md_data="0x2b" type="CHAR">
            <ptl_md md_data="0xc9" type="CHAR">
              <ptl_swap atom_op="CSWAP_LE" atom_type="CHAR" length="1" operand="0x9c" target_id="SELF"/>
              <msleep count="50"/>
              <check length="1" md_data="0x6d" type="CHAR"/>
              <check length="1" md_data="0xc9" offset="1" type="CHAR"/>
            </ptl_md>
            <check length="1" md_data="0x2b" type="CHAR"/>
            <check length="1" md_data="0x2b" offset="1" type="CHAR"/>
          </ptl_md>
          <check le_data="0x2b" length="1" type="CHAR"/>
          <check le_data="0x6d" length="1" offset="1" type="CHAR"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

<?xml version="1.0"?>
<test>
  <desc>Test atomic PROD/FLOAT length=4</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0.3579230011" le_opt="OP_PUT" type="FLOAT">
          <ptl_md md_data="0.3702368438" type="FLOAT">
            <ptl_atomic atom_op="PROD" atom_type="FLOAT" length="4" target_id="SELF"/>
            <msleep count="50"/>
            <check length="4" md_data="0.3702368438" type="FLOAT"/>
            <check length="4" md_data="0.3702368438" offset="4" type="FLOAT"/>
          </ptl_md>
          <check le_data="0.1325162798" length="4" type="FLOAT"/>
          <check le_data="0.3579230011" length="4" offset="4" type="FLOAT"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

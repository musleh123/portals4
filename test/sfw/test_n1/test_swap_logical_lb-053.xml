<?xml version="1.0"?>
<test>
  <desc>Test swap CSWAP_NE/USHORT</desc>
  <ptl>
    <ptl_ni>
      <ompi_rt>
        <ptl_ni ni_opt="NO_MATCH LOGICAL">
          <ptl_pt>
            <ptl_le le_data="0xcc51" le_opt="OP_PUT OP_GET" type="USHORT">
              <ptl_md md_data="0x9c51" type="USHORT">
                <ptl_md md_data="0x4267" type="USHORT">
                  <ptl_swap atom_op="CSWAP_NE" atom_type="USHORT" length="2" operand="0x2c12" target_id="SELF"/>
                  <msleep count="50"/>
                </ptl_md>
                <check length="2" md_data="0x9c51" type="USHORT"/>
                <check length="2" md_data="0x9c51" offset="2" type="USHORT"/>
              </ptl_md>
              <check le_data="0x9c51" length="2" type="USHORT"/>
              <check le_data="0xcc51" length="2" offset="2" type="USHORT"/>
            </ptl_le>
          </ptl_pt>
          <barrier/>
        </ptl_ni>
      </ompi_rt>
    </ptl_ni>
  </ptl>
</test>

<?xml version="1.0"?>
<test>
  <desc>Test atomic SUM/UINT8 length=20</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0xe1" le_opt="OP_PUT" type="UINT8">
          <ptl_md md_data="0x87" type="UINT8">
            <ptl_atomic atom_op="SUM" atom_type="UINT8" length="20" target_id="SELF"/>
            <msleep count="50"/>
            <check length="20" md_data="0x87" type="UINT8"/>
            <check length="1" md_data="0x87" offset="20" type="UINT8"/>
          </ptl_md>
          <check le_data="0x68" length="20" type="UINT8"/>
          <check le_data="0xe1" length="1" offset="20" type="UINT8"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

<?xml version="1.0"?>
<test>
  <desc>Test atomic MAX/UINT32 length=16</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0x5a940357" le_opt="OP_PUT" type="UINT32">
          <ptl_md md_data="0x05b98b82" type="UINT32">
            <ptl_atomic atom_op="MAX" atom_type="UINT32" length="16" target_id="SELF"/>
            <msleep count="50"/>
            <check length="16" md_data="0x05b98b82" type="UINT32"/>
            <check length="4" md_data="0x05b98b82" offset="16" type="UINT32"/>
          </ptl_md>
          <check le_data="0x5a940357" length="16" type="UINT32"/>
          <check le_data="0x5a940357" length="4" offset="16" type="UINT32"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

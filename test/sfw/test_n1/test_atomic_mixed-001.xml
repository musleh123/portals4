<?xml version="1.0"?>
<test>
  <desc>Test atomic BXOR/INT8 length=18</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0x0e" le_opt="OP_PUT" type="INT8">
          <ptl_md md_data="0x24" type="INT8">
            <ptl_atomic atom_op="BXOR" atom_type="INT8" length="18" target_id="SELF"/>
            <msleep count="50"/>
            <check length="18" md_data="0x24" type="INT8"/>
            <check length="1" md_data="0x24" offset="18" type="INT8"/>
          </ptl_md>
          <check le_data="0x2a" length="18" type="INT8"/>
          <check le_data="0x0e" length="1" offset="18" type="INT8"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

<?xml version="1.0"?>
<test>
  <desc>Test swap CSWAP_NE/UCHAR</desc>
  <ptl>
    <ptl_ni>
      <ompi_rt>
        <ptl_ni ni_opt="MATCH LOGICAL">
          <ptl_pt>
            <ptl_me me_data="0x42" me_match="0x7f12bb0" me_opt="OP_PUT OP_GET" type="UCHAR">
              <ptl_md md_data="0x55" type="UCHAR">
                <ptl_md md_data="0xaf" type="UCHAR">
                  <ptl_swap atom_op="CSWAP_NE" atom_type="UCHAR" length="1" match="0x7f12bb0" operand="0x6e" target_id="SELF"/>
                  <msleep count="50"/>
                </ptl_md>
                <check length="1" md_data="0x55" type="UCHAR"/>
                <check length="1" md_data="0x55" offset="1" type="UCHAR"/>
              </ptl_md>
              <check length="1" me_data="0x55" type="UCHAR"/>
              <check length="1" me_data="0x42" offset="1" type="UCHAR"/>
            </ptl_me>
          </ptl_pt>
          <barrier/>
        </ptl_ni>
      </ompi_rt>
    </ptl_ni>
  </ptl>
</test>

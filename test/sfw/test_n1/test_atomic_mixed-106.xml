<?xml version="1.0"?>
<test>
  <desc>Test atomic BAND/UINT length=16</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0x23d6d6ca" le_opt="OP_PUT" type="UINT">
          <ptl_md md_data="0x04f8eff2" type="UINT">
            <ptl_atomic atom_op="BAND" atom_type="UINT" length="16" target_id="SELF"/>
            <msleep count="50"/>
            <check length="16" md_data="0x04f8eff2" type="UINT"/>
            <check length="4" md_data="0x04f8eff2" offset="16" type="UINT"/>
          </ptl_md>
          <check le_data="0x00d0c6c2" length="16" type="UINT"/>
          <check le_data="0x23d6d6ca" length="4" offset="16" type="UINT"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

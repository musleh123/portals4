<?xml version="1.0"?>
<test>
  <desc>Test atomic MAX/INT32 length=32</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0x240210ab" le_opt="OP_PUT" type="INT32">
          <ptl_md md_data="0x5eabf4f8" type="INT32">
            <ptl_atomic atom_op="MAX" atom_type="INT32" length="32" target_id="SELF"/>
            <msleep count="50"/>
            <check length="32" md_data="0x5eabf4f8" type="INT32"/>
            <check length="4" md_data="0x5eabf4f8" offset="32" type="INT32"/>
          </ptl_md>
          <check le_data="0x5eabf4f8" length="32" type="INT32"/>
          <check le_data="0x240210ab" length="4" offset="32" type="INT32"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

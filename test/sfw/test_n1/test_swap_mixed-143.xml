            </ptl_md>
            <check length="1" md_data="0x85" type="UINT8"/>
            <check length="1" md_data="0x85" offset="1" type="UINT8"/>
          </ptl_md>
          <check le_data="0x85" length="1" type="UINT8"/>
          <check le_data="0xf7" length="1" offset="1" type="UINT8"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>

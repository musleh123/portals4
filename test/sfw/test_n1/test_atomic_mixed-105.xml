<?xml version="1.0"?>
<test>
  <desc>Test atomic LXOR/INT32 length=8</desc>
  <ptl>
    <ptl_ni ni_opt="MATCH PHYSICAL">
      <ptl_pt>
        <ptl_me me_data="0x35e16de9" me_match="0x7435736077854929941" me_opt="OP_PUT" type="INT32">
          <ptl_md md_data="0x45fca2e0" type="INT32">
            <ptl_atomic atom_op="LXOR" atom_type="INT32" length="8" match="0x7435736077854929941" target_id="SELF"/>
            <msleep count="50"/>
            <check length="8" md_data="0x45fca2e0" type="INT32"/>
            <check length="4" md_data="0x45fca2e0" offset="8" type="INT32"/>
          </ptl_md>
          <check length="8" me_data="0x00000000" type="INT32"/>
          <check length="4" me_data="0x35e16de9" offset="8" type="INT32"/>
        </ptl_me>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>

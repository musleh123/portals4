<?xml version="1.0"?>
<test>
  <desc>Test swap CSWAP_LT/CHAR</desc>
  <ptl>
    <ptl_ni>
      <ompi_rt>
        <ptl_ni ni_opt="MATCH LOGICAL">
          <ptl_pt>
            <ptl_me me_data="0xc7" me_match="0x54fc0cf" me_opt="OP_PUT OP_GET" type="CHAR">
              <ptl_md md_data="0xab" type="CHAR">
                <ptl_md md_data="0x20" type="CHAR">
                  <ptl_swap atom_op="CSWAP_LT" atom_type="CHAR" length="1" match="0x54fc0cf" operand="0x44" target_id="SELF"/>
                  <msleep count="50"/>
                </ptl_md>
                <check length="1" md_data="0xab" type="CHAR"/>
                <check length="1" md_data="0xab" offset="1" type="CHAR"/>
              </ptl_md>
              <check length="1" me_data="0xc7" type="CHAR"/>
              <check length="1" me_data="0xc7" offset="1" type="CHAR"/>
            </ptl_me>
          </ptl_pt>
          <barrier/>
        </ptl_ni>
      </ompi_rt>
    </ptl_ni>
  </ptl>
</test>

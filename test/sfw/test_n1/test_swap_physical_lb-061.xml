<?xml version="1.0"?>
<test>
  <desc>Test swap CSWAP_LT/UCHAR</desc>
  <ptl>
    <ptl_ni ni_opt="NO_MATCH PHYSICAL">
      <ptl_pt>
        <ptl_le le_data="0xf1" le_opt="OP_PUT OP_GET" type="UCHAR">
          <ptl_md md_data="0xb8" type="UCHAR">
            <ptl_md md_data="0x06" type="UCHAR">
              <ptl_swap atom_op="CSWAP_LT" atom_type="UCHAR" length="1" operand="0x36" target_id="SELF"/>
              <msleep count="50"/>
            </ptl_md>
            <check length="1" md_data="0xb8" type="UCHAR"/>
            <check length="1" md_data="0xb8" offset="1" type="UCHAR"/>
          </ptl_md>
          <check le_data="0xb8" length="1" type="UCHAR"/>
          <check le_data="0xf1" length="1" offset="1" type="UCHAR"/>
        </ptl_le>
      </ptl_pt>
    </ptl_ni>
  </ptl>
</test>
